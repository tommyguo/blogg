{
  "name": "blogg",
  "version": "1.0.0",
  "description": "Blogg is a build-your-own blog web app that uses Express, Webpack, React, and React-Router.",
  "scripts": {
    "buildDev": "rm -rf dist && webpack --mode development --config src/config/webpack.server.config.js && webpack --mode development --config src/config/webpack.apiServer.config.js && webpack --mode development --config src/config/webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config src/config/webpack.server.config.js && webpack --mode production --config src/config/webpack.apiServer.config.js && webpack --mode production --config src/config/webpack.prod.config.js",
    "start": "node ./dist/server.js & node ./dist/apiServer.js"
  },
  "repository": "https://github.com/tommyguo/blogg",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.3",
    "google-auth-library": "^4.2.2",
    "http-proxy": "^1.17.0",
    "multer": "^1.4.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.0.1",
    "remarkable": "^1.7.1",
    "sequelize": "^6.0.0",
    "serve-favicon": "^2.5.0"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.12.1",
    "file-loader": "^1.1.11",
